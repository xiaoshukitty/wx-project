.tabbar {
    z-index: 100;
    width: 100%;
    position: fixed;
    bottom: 0;
    font-size: 28rpx;
    background-color: #fff;
    color: #636363;
    border-radius: 50rpx 50rpx 0px 0px;
}

.navigation {
    z-index: 0;
    height: calc(100rpx + env(safe-area-inset-bottom) / 2);
    padding-bottom: calc(env(safe-area-inset-bottom) / 2);
    padding-bottom: 30rpx;
    display: flex;
    justify-content: space-around;
}

.navigation .btn {
    width: 20%;
    font-size: 24rpx;
    position: relative;
    text-align: center;
    padding: 0;
    display: block;
    height: auto;
    line-height: 1;
    margin: 0;
    background-color: inherit;
    overflow: initial;
    justify-content: center;
    align-items: center;
    padding-top: 20rpx;
}

